                                    $(".menu-container a").eq(i).addClass("activeTab");
                                }
                            });
                        })
                        .scroll();
                }
                else {
                    //script for go to top in mobile
                    $("#toTop").click(function () {
                        $("html, body").animate({ scrollTop: 0 }, 2000);
                    });
                }
            });
        });

